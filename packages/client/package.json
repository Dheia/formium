{
  "name": "@formium/client",
  "version": "0.0.18",
  "description": "Autogenerate IDs to facilitate WAI-ARIA and server rendering.",
  "author": "Jared Palmer <hello@formium.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formium/formium.git",
    "directory": "packages/client"
  },
  "scripts": {
    "start": "tsdx watch --format=esm,cjs,umd --name Formium --tsconfig tsconfig.build.json  --verbose --noClean --onSuccess \"yalc push\"",
    "build": "tsdx build --format=esm,cjs,umd --name Formium --tsconfig tsconfig.build.json",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepublish": "npm run build"
  },
  "main": "dist/index.js",
  "module": "dist/client.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "dependencies": {
    "@formium/utils": "^0.0.18",
    "cross-fetch": "^3.0.5",
    "query-string": "^6.13.1",
    "tslib": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
